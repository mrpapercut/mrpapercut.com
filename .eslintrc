{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "describe": true,
    "it": true,
    "xit": true
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "mocha": true
  },
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "brace-style": 2,
    "camelcase": [2, {"properties": "always"}],
    "comma-dangle": [2, "never"],
    "comma-style": [2, "last"],
    "consistent-this": 2,
    "constructor-super": 2,
    "curly": [2, "multi-line"],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 0,
    "indent": [2, "tab", {"SwitchCase": 1}],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "keyword-spacing": [2],
    "linebreak-style": [2, "unix"],
    "new-cap": 2,
    "no-case-declarations": 2,
    "no-const-assign": 2,
    "no-control-regex": [2],
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": [2, {"exceptions": {"ImportDeclaration": true, "VariableDeclarator": true}}],
    "no-multi-str": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-self-assign": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-labels": 2,
    "no-unused-vars": 2,
    "no-var": 1,
    "object-shorthand": 1,
    "prefer-const": [2],
    "quotes": 0,
    "spaced-comment": [2, "always"],
    "space-infix-ops": 2,
    "space-in-parens": [2, "never"],
    "space-unary-ops": [2, {"words": false, "nonwords": false}],
    "strict": 0,
    "wrap-iife": 2,
    "yoda": [2, "never"]
  }
}
